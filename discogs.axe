{
  "manifest": {
    "id": "discogs",
    "name": "Discogs",
    "type": "meta-service",
    "version": "1.0.0",
    "author": "Parachord Team",
    "description": "Artist biographies and images from Discogs",
    "icon": "ðŸ“€",
    "color": "#333333",
    "homepage": "https://www.discogs.com"
  },
  "capabilities": {
    "metadata": true,
    "biography": true,
    "artistImages": true
  },
  "settings": {
    "requiresAuth": false,
    "authType": "token",
